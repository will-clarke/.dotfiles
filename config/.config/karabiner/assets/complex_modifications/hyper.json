{
  "title": "!hyper launcher",
  "rules": [
    {
      "description": "hyper-space to emacs",
      "manipulators": [
        {
          "from": {
            "key_code": "spacebar",
            "modifiers": {
              "mandatory": [
                "shift",
                "control",
                "option",
                "command"
              ]
            }
          },
          "to": [
            {
              "shell_command": "open -a emacs"
            }
          ],
          "type": "basic"
        }
      ]
    },
    {
      "description": "hyper-e to emacs",
      "manipulators": [
        {
          "from": {
            "key_code": "e",
            "modifiers": {
              "mandatory": [
                "command",
                "shift",
                "control",
                "option"
              ],
              "optional": [
                "caps_lock"
              ]
            }
          },
          "to": [
            {
              "shell_command": "open -a emacs"
            }
          ],
          "type": "basic"
        }
      ]
    },
    {
      "description": "hyper-c to chrome",
      "manipulators": [
        {
          "from": {
            "key_code": "c",
            "modifiers": {
              "mandatory": [
                "command",
                "shift",
                "control",
                "option"
              ],
              "optional": [
                "caps_lock"
              ]
            }
          },
          "to": [
            {
              "shell_command": "open -a \"Google Chrome\""
            }
          ],
          "type": "basic"
        }
      ]
    },

    {
      "description": "hyper-q to zulip",
      "manipulators": [
        {
          "from": {
            "key_code": "q",
            "modifiers": {
              "mandatory": [
                "command",
                "shift",
                "control",
                "option"
              ]
            }
          },
          "to": [
            {
              "shell_command": "open -a \"zulip\""
            }
          ],
          "type": "basic"
        }
      ]
    },
    {
      "description": "hyper-w to wally",
      "manipulators": [
        {
          "from": {
            "key_code": "w",
            "modifiers": {
              "mandatory": [
                "left_alt",
                "left_control",
                "left_gui",
                "left_shift",
                "command",
                "shift",
                "control",
                "option"
              ],
              "optional": [
                "caps_lock"
              ]
            }
          },
          "to": [
            {
              "shell_command": "open -a \"wally\""
            }
          ],
          "type": "basic"
        }
      ]
    },
    {
      "description": "hyper-v to zoom (video)",
      "manipulators": [
        {
          "from": {
            "key_code": "v",
            "modifiers": {
              "mandatory": [
                "command",
                "shift",
                "control",
                "option"
              ],
              "optional": [
                "caps_lock"
              ]
            }
          },
          "to": [
            {
              "shell_command": "open -a \"zoom.us\""
            }
          ],
          "type": "basic"
        }
      ]
    },
    {
      "description": "hyper-a to alacritty",
      "manipulators": [
        {
          "from": {
            "key_code": "a",
            "modifiers": {
              "mandatory": [
                "command",
                "shift",
                "control",
                "option"
              ],
              "optional": [
                "caps_lock"
              ]
            }
          },
          "to": [
            {
              "shell_command": "open -a \"alacritty\""
            }
          ],
          "type": "basic"
        }
      ]
    }
  ]
}
